{
  "name": "coffee-sprites",
  "version": "0.0.8",
  "description": "CoffeeScript/JavaScript stylesheet-based sprite image engine.",
  "main": "js/coffee-sprites.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "pretest": "coffee --bare -o js coffee/coffee-sprites.coffee",
    "test": "rm -rf test/fixtures/static/public/assets/* && mocha --timeout 100000 --reporter spec --bail --ui bdd --colors --compilers coffee:coffee-script --require coffee-script test/test.coffee"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mikesmullin/coffee-sprites.git"
  },
  "keywords": [
    "coffee",
    "css",
    "stylesheet",
    "stylesheets",
    "image",
    "sprite",
    "sprites",
    "spriting",
    "plugin",
    "fast",
    "engine"
  ],
  "author": "Mike Smullin <mike@smullindesign.com>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "devDependencies": {
    "chai": "*",
    "coffee-stylesheets": "*"
  },
  "dependencies": {
    "node-gd": "*",
    "async2": "*",
    "glob": "*"
  }
}
